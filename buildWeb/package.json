{
  "name": "build_web",
  "version": "1.0.0",
  "scripts": {
    "lang": "node ./lib/lang.js",
    "build": "rimraf .dist && node ./lib/build.js",
    "watch": "chokidar \"src/**/*\" \"lang/**/*\" -c \"node ./lib/build.js\"",
    "dev": "concurrently \"npm run watch\" \"vite\""
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "chokidar-cli": "^3.0.0",
    "clean-css": "^5.3.3",
    "concurrently": "^9.2.0",
    "express": "^5.1.0",
    "nunjucks": "^3.2.4",
    "sass": "^1.90.0",
    "vite": "^7.1.2",
    "xlsx": "^0.18.5"
  }
}